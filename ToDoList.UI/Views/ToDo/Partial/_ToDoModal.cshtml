@{
    Layout = null;
}
<button type="button" id="btnModal" hidden="hidden" class="btn btn-primary" style="display: none;" data-toggle="modal" data-target="#exampleModal">
</button>

<!-- Modal -->
<div v-if="timeToDoData != null" class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel"> <strong>Warning - To Do List</strong></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div v-for="(row,index) in timeToDoData">
                    <p><strong>Id:</strong> {{row.Id}}</p>
                    <p><strong>Name:</strong> {{row.Name}}</p>
                    <p><strong>Description: </strong>{{row.Description}}</p>
                    <hr />
                </div>
                

            </div>
        </div>
    </div>
</div>